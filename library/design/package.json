{
  "private": true,
  "type": "module",
  "name": "@library/design",
  "version": "0.0.1",
  "description": "List components",
  "author": "Viktor Pyatakov <pyatakov.viktor@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf lib types",
    "build": "echo '>-Start build \"@library/design\"->' && yarn run clean && yarn run build:compile && yarn run build:types && echo '<-Finish build \"@library/design\"-<'",
    "build:types": "echo 'Building types \"@library/design\"' && cross-env NODE_ENV=production tsc --project tsconfig.json",
    "build:compile": "echo 'Build package \"@library/design\"' && cross-env NODE_ENV=production babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files",
    "start": "npm-run-all --parallel start:watch start:types",
    "start:types": "tsc-watch --noClear --noColors",
    "start:watch": "cross-env NODE_ENV=development babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files --watch"
  },
  "dependencies": {
    "@library/app": "0.0.1",
    "@library/breadcrumbs": "0.0.1",
    "@library/kit": "0.0.1",
    "classnames": "2.5.1",
    "moment": "2.30.1",
    "moment-timezone": "0.5.45",
    "react": "18.3.1",
    "react-router-dom": "6.23.1"
  },
  "devDependencies": {
    "@babel/cli": "7.24.7",
    "@babel/core": "7.24.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.24.7",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-transform-class-properties": "7.25.4",
    "@babel/plugin-transform-private-methods": "7.25.4",
    "@babel/plugin-transform-private-property-in-object": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/preset-react": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@types/moment": "2.13.0",
    "babel-plugin-module-resolver": "5.0.2",
    "cross-env": "7.0.3",
    "npm-run-all": "4.1.5",
    "rimraf": "5.0.7",
    "tsc": "2.0.4",
    "tsc-watch": "6.2.0",
    "typescript": "5.5.2"
  }
}
